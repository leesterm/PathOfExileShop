<h1>{{item.0.item_name}}</h1>
<img src="/images/{{item.0.item_image}}"/>
<div>SELLER: {{item.0.username}}</div>
<div>STATUS: {{item.0.status}}</div>
<div>ITEM LEVEL: {{item.0.item_level}}</div>
<span>STR {{item.0.str_req}}</span>
<span>DEX {{item.0.dex_req}}</span>
<span>INT {{item.0.int_req}}</span>
<div>COST: {{item.0.cost}} Exalted Orbs</div>
<h2>Affixes</h2>
{{#each item}}
	<div>{{this.name}}: {{this.description}}</div>
{{/each}}
